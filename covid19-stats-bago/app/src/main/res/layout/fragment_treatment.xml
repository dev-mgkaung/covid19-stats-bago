<?xml version="1.0" encoding="utf-8"?>
<androidx.swiperefreshlayout.widget.SwipeRefreshLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/treatment_swiperefresh"
    tools:context=".ui.returnstats.ReturnedPeopleFragment">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <io.supercharge.shimmerlayout.ShimmerLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:shimmer_color="#E0E0E0"
            android:id="@+id/treatment_shimmerlayout">
            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <gov.mm.covid19statsbago.mmfont.components.MMTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="16sp"
                        android:layout_margin="10dp"
                        android:maxLines="1"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textColor="#353434"
                        android:text="လုပ်ဆောင်နေပါသည်ခေတ္တခဏစောင့်ဆိုင်းပေးပါ"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_weight="3"
                        android:layout_height="match_parent">
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_weight="3"
                        android:layout_height="match_parent">
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_weight="3"
                        android:layout_height="match_parent">
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_weight="3"
                        android:layout_height="match_parent">
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_weight="3"
                        android:layout_height="match_parent">
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_weight="3"
                        android:layout_height="match_parent">
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_weight="3"
                        android:layout_height="match_parent">
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                    </LinearLayout>
                </LinearLayout>
            </ScrollView>
        </io.supercharge.shimmerlayout.ShimmerLayout>


        <com.evrencoskun.tableview.TableView
            android:id="@+id/treatment_table_view"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:column_header_height="@dimen/table_view_cell_height"
            app:row_header_width="@dimen/table_view_row_header_width"
            app:selected_color="@color/selected_background_color"
            app:separator_color="@color/separator_color"
            app:shadow_color="@color/shadow_background_color"
            app:unselected_color="@color/unselected_background_color" />


    </RelativeLayout>
</androidx.swiperefreshlayout.widget.SwipeRefreshLayout>