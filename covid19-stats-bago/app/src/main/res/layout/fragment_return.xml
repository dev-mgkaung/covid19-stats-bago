<?xml version="1.0" encoding="utf-8"?>
<androidx.swiperefreshlayout.widget.SwipeRefreshLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/return_swiperefresh"
    tools:context=".ui.returnstats.ReturnedPeopleFragment">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <io.supercharge.shimmerlayout.ShimmerLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:shimmer_color="#E0E0E0"
            android:id="@+id/return_shimmerlayout">
            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <gov.mm.covid19statsbago.mmfont.components.MMTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="16sp"
                        android:layout_margin="10dp"
                        android:maxLines="1"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textColor="#353434"
                        android:text="လုပ်ဆောင်နေပါသည်ခေတ္တခဏစောင့်ဆိုင်းပေးပါ"/>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_weight="3"
                        android:layout_height="match_parent">
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_weight="3"
                        android:layout_height="match_parent">
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_weight="3"
                        android:layout_height="match_parent">
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_weight="3"
                        android:layout_height="match_parent">
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_weight="3"
                        android:layout_height="match_parent">
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_weight="3"
                        android:layout_height="match_parent">
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_weight="3"
                        android:layout_height="match_parent">
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:layout_height="80dp"
                            android:layout_margin="5dp"
                            android:background="@drawable/group_bg_grey"/>
                    </LinearLayout>
                </LinearLayout>
            </ScrollView>
        </io.supercharge.shimmerlayout.ShimmerLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:visibility="gone"
            android:id="@+id/returnpeoplelayout">

        <gov.mm.covid19statsbago.mmfont.components.MMTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_margin="10dp"
            android:lineSpacingExtra="5dp"
            android:textSize="12sp"
            android:textColor="#000000"
            android:text="ပဲခူးတိုင်းဒေသကြီးအတွင်း COVID-19 နှင့်ပတ်သက်၍ ပြည်ပနိုင်ငံများမှ ပြန်လည်ရောက်ရှိလာသူများစာရင်း"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="1sp"
            android:background="#E0E0E0"
            android:layout_margin="2dp"/>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_height="wrap_content">
                <gov.mm.covid19statsbago.mmfont.components.MMTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:id="@+id/returngetalllist"
                    android:layout_margin="10dp"
                    android:layout_alignParentLeft="true"
                    android:textSize="14sp"
                    android:drawableLeft="@drawable/ic_filter_list_black_24dp"
                    android:textColor="#3949AB"
                    android:text="ALL Data"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:gravity="right"
                    android:orientation="horizontal">

                    <gov.mm.covid19statsbago.mmfont.components.MMTextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_margin="10dp"
                        android:textSize="13sp"
                        android:textColor="#000000"
                        android:text="ရက်ရွေးရန်"/>

                    <gov.mm.covid19statsbago.mmfont.components.MMTextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:layout_margin="10dp"
                        android:text="00/00/0000"
                        android:id="@+id/returndatechoose"
                        android:textSize="11sp"
                        android:padding="10dp"
                        android:background="@drawable/rounded_bg_background_date"
                        android:textColor="@color/colorPrimary"/>

                </LinearLayout>
            </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1sp"
            android:background="#E0E0E0"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="2dp"
            android:layout_marginRight="2dp"/>

    <com.evrencoskun.tableview.TableView
        android:id="@+id/returnpeople_table_view"
        android:visibility="visible"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:column_header_height="80dp"
        app:row_header_width="@dimen/table_view_row_header_width"
        app:selected_color="@color/selected_background_color"
        app:separator_color="@color/separator_color"
        app:shadow_color="@color/shadow_background_color"
        app:unselected_color="@color/unselected_background_color" />
        </LinearLayout>


    </RelativeLayout>
</androidx.swiperefreshlayout.widget.SwipeRefreshLayout>